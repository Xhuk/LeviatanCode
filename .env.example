# Database Configuration
DATABASE_URL=postgresql://postgres.xxxxx:your-password@aws-0-region.pooler.supabase.com:6543/postgres

# AI Services (choose one or both)
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GEMINI_API_KEY=AIzaSyxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# AI Model and Budget Tracking
# Default OpenAI model to use for completions
OPENAI_MODEL=gpt-4o
# Number of tokens allowed before billing starts
AI_FREE_TOKENS=100000
# Maximum allowed spend in USD before halting requests
AI_BUDGET_LIMIT=5.00
# Allow requests beyond the budget limit when set to true
AI_BUDGET_OVERRIDE=false

# Application Configuration
NODE_ENV=development
PORT=5005
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# Optional: For enhanced security
BCRYPT_ROUNDS=10
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100